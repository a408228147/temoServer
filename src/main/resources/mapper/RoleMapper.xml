<mapper namespace="com.creams.temo.mapper.RoleMapper">

    <insert id="addRole">
       INSERT INTO role (user_id, role_id, role_name)
       VALUES
       (#{userId},#{roleId}, #{roleName})
    </insert>

</mapper>